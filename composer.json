{
  "name": "vleroy/smart",
  "description": "Laravel Smart",
  "license": "MIT",
  "authors": [
    {
      "name": "Andrei Canta",
      "email": "deiucanta@gmail.com"
    },
    {
      "name": "Valentin LEROY",
      "email": "contact@vleroy.fr"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "illuminate/support": "^6.0",
    "doctrine/dbal": "^2.8",
    "illuminate/validation": "^6.0",
    "illuminate/console": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "Deiucanta\\Smart\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Deiucanta\\Smart\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit --colors=always"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Deiucanta\\Smart\\SmartServiceProvider"
      ]
    }
  },
  "require-dev": {
    "orchestra/testbench": "^3.8@dev",
    "phpunit/phpunit": "^7.4@dev"
  }
}
